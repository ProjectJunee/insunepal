generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x", "darwin-arm64", "rhel-openssl-3.0.x"]
  // output        = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model LifeInsuranceQuote {
  id             Int    @id @default(autoincrement())
  firstName      String
  lastName       String
  district       String
  phone          String
  dateOfBirth    Int
  gender         Gender
  email          String @unique
  coverageAmount Int    @default(500000)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Gender {
  male
  female
  other
}
